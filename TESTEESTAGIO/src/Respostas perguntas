1 -) public class Main {
         public static void main(String[] args) {

             int indice = 13;
             int SOMA = 0;
             int K = 0;

             while (K < indice) {
                 K = K + 1;
                 SOMA = SOMA + K;

             }

             System.out.println(SOMA);      RESULTADO 91
         }
     }



